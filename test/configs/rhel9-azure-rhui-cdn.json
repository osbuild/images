{
  "name": "rhel9-azure-rhui-cdn",
  "blueprint": {
    "name": "rhel9-azure-rhui-cdn",
    "description": "Replicates the pungi build config for the RHEL 9 Azure images with RHUI installed and the CDN cloud client config.",
    "version": "1.0",
    "packages": [
      {
        "name": "rhui-azure-rhel9"
      },
      {
        "name": "redhat-cloud-client-configuration-cdn"
      }
    ],
    "customizations": {
      "rpm": {
        "import_keys": {
          "files": [
            "/etc/pki/rpm-gpg/RPM-GPG-KEY-microsoft-azure-release"
          ]
        }
      },
      "rhsm": {
        "config": {
          "subscription_manager": {
            "rhsm": {
              "auto_enable_yum_plugins": false
            }
          }
        }
      }
    }
  }
}
