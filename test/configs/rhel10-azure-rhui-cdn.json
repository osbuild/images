{
  "name": "rhel10-azure-rhui-cdn",
  "blueprint": {
    "name": "rhel10-azure-rhui-cdn",
    "description": "Replicates the pungi build config for the RHEL 10 Azure images with RHUI installed and the CDN cloud client config.",
    "version": "1.0",
    "packages": [
      {
        "name": "rhui-azure-rhel10"
      },
      {
        "name": "redhat-cloud-client-configuration-cdn"
      }
    ],
    "customizations": {
      "rhsm": {
        "config": {
          "subscription_manager": {
            "rhsm": {
              "auto_enable_yum_plugins": false
            }
          }
        }
      }
    }
  }
}
