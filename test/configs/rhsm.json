{
  "name": "rhsm",
  "blueprint": {
    "name": "rhsm",
    "description": "Simple blueprint with subscription manager options.",
    "version": "1.0",
    "customizations": {
      "rhsm": {
        "config": {
          "dnf_plugins": {
            "product_id": {
              "enabled": true
            },
            "subscription_manager": {
              "enabled": true
            }
          },
          "subscription_manager": {
            "rhsm": {
              "manage_repos": true
            },
            "rhsmcertd": {
              "auto_registration": true
            }
          }
        }
      }
    }
  }
}
