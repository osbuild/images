{
  "name": "firewall",
  "blueprint": {
    "name": "firewall",
    "description": "Simple blueprint with firewall customizations.",
    "version": "1.0",
    "customizations": {
      "firewall": {
        "ports": [
          "1080:tcp"
        ],
        "services": {
          "enabled": [
            "ssh"
          ],
          "disabled": [
            "http"
          ]
        }
      }
    }
  }
}
